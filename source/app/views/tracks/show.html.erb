<head>
<link rel="stylesheet" type="text/css" href="../assets/track.css">

</head>
<body>

<div id="track-title">
  <h1><strong><%= @track.title%></strong></h1>
</div>

<% @sections.each do |section| %>
    <div class="section-title">
      <h2><strong> <%= section.title %> </strong></h2>
    </div>

    <div class="section-and-submission-container">
      <div class="section-container" data-section-id="<%=section.id%>">
        <%= render 'sections/section_resources', section: section %>
      </div>
      <%= render 'sections/submit_resource', section: section%>
    </div>
<% end %>

</body>